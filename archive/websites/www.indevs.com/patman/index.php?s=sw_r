   function CheckShout(form)
   {
	if ((form.shoutname.value == '') || (form.shoutname.value.toUpperCase() == "NAME HERE"))
	{
		alert("Enter a shout name");
		return false;
	}
	else if (form.shoutname.value.length > 20)
	{
		alert("Max shout name length is 20 characters\nCurrent length is " + shoutname.value.length + " characters");
		return false;
	}
	else if (
		(form.shoutname.value.indexOf('<') != -1) ||
		(form.shoutname.value.indexOf('>') != -1) ||
		(form.shoutname.value.indexOf('%') != -1) ||
		(form.shoutname.value.indexOf('&') != -1) ||
		(form.shoutname.value.indexOf(';') != -1) ||
		(form.shoutname.value.indexOf(':') != -1) ||
		(form.shoutname.value.indexOf('=') != -1)
		)
	{
		alert("Illegal character(s) found in shout name\nIllegal characters are: <>%&;:=");
		return false;
	}

	if ((form.shoutmsg.value == '') || (form.shoutmsg.value.toUpperCase() == "MESSAGE HERE"))
	{
		alert("Enter a shout message");
		return false;
	}
	else if (form.shoutmsg.value.length > 200)
	{
		alert("Max shout message length is 200 characters\nCurrent length is " + shoutmsg.value.length + " characters");
		return false;
	}
	else if (
		(form.shoutmsg.value.indexOf('<') != -1) ||
		(form.shoutmsg.value.indexOf('>') != -1) ||
		(form.shoutmsg.value.indexOf('%') != -1) ||
		(form.shoutmsg.value.indexOf('&') != -1) ||
		(form.shoutmsg.value.indexOf(';') != -1) ||
		(form.shoutmsg.value.indexOf(':') != -1) ||
		(form.shoutmsg.value.indexOf('=') != -1)
		)
	{
		alert("Illegal character(s) found in shout message\nIllegal characters are: <>%&;:=");
		return false;
	}

	return true;
   }
